.body {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  background-image: url('https://wx.luckydeer.cn/static/images/bg_img_movie/login_background.jpg');
  background-repeat: no-repeat;
  background-size: 100%;
}

.defaultInfo {
  text-align: center;
  border-style: solid;
  border-color: red;
  border-width: 2rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
}

